   3.924768290684769*^9, 3.924768298039879*^9}, {3.9249410247826076`*^9, 
   3.924941025804656*^9}, 3.924941350428367*^9, {3.924941465348573*^9, 
   3.9249414660209055`*^9}, {3.925281885009523*^9, 3.925281886058878*^9}, {
   3.925284210071289*^9, 3.925284210158304*^9}, 3.925287168186694*^9, 
   3.9252876106592507`*^9, {3.926575613995171*^9, 3.926575620315405*^9}, {
