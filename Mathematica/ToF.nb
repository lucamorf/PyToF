Cell[TextData[{
 "Hence\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["V", "TI"], 
          StyleBox["ext", FontSlant -> "Plain"]], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["r", "TI"], ",", "\[CurlyTheta]"}], ")"}], 
         "\[LongEqual]", "-", 
         FractionBox[
          StyleBox["G", "TI"], 
          StyleBox["r", "TI"]], 
         UnderoverscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["n", "TI"], "\[LongEqual]", "0"}], "\[Infinity]", 
          LimitsPositioning -> True], 
         SuperscriptBox[
          StyleBox["r", "TI"], 
          RowBox[{"-2", 
            StyleBox["n", "TI"]}]], 
         SubscriptBox[
          StyleBox["D", "TI"], 
          RowBox[{"2", 
            StyleBox["n", "TI"]}]], 
         RowBox[{"(", 
           StyleBox["r", "TI"], ")"}], 
         SubscriptBox[
          StyleBox["P", "TI"], 
          RowBox[{"2", 
            StyleBox["n", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{"cos", "\[CurlyTheta]"}], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$V_\\mathrm{ext}(r, \\vartheta)=-\\frac{G}{r} \
\\sum_{n=0}^{\\infty}r^{-2 n} D_{2 n}(r)P_{2 n}(\\cos\\vartheta)$", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ef4c3033-face-004c-9256-f00b7f9d96d1"],
 ".\nRemembering ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["S", "TI"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", 
           StyleBox["z", "TI"], ")"}], "\[LongEqual]", 
         FractionBox["3", 
          RowBox[{"4", "\[Pi]", 
            OverscriptBox["\[Rho]", "_"], 
            SuperscriptBox[
             StyleBox["l", "TI"], 
             RowBox[{
               StyleBox["n", "TI"], "+", "3"}]]}]], 
         SubscriptBox[
          StyleBox["D", "TI"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", 
           StyleBox["l", "TI"], ")"}]}], TraditionalForm], "errors" -> {}, 
     "input" -> "$S_n(z)=\\frac{3}{4 \\pi \\bar{\\rho} l^{n+3}} D_n(l)$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "009b8d4a-3ada-e040-9ed5-436d6444de73"],
 ", ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["S", "TI"], "0"], 
         RowBox[{"(", 
           StyleBox["z", "TI"], ")"}], "\[LongEqual]", 
         FractionBox[
          RowBox[{
            StyleBox["m", "TI"], "(", 
            StyleBox["z", "TI"], ")"}], 
          RowBox[{
            StyleBox["M", "TI"], 
            SuperscriptBox[
             StyleBox["z", "TI"], "3"]}]]}], TraditionalForm], "errors" -> {},
      "input" -> "$S_0(z)=\\frac{m(z)}{M z^3}$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "8edca82f-a46b-5448-9325-6f3f38d078ac"],
 ", ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["l", "TI"], "\[LongEqual]", 
         StyleBox["z", "TI"], 
         SubscriptBox[
          StyleBox["R", "TI"], 
          StyleBox["m", FontSlant -> "Plain"]]}], TraditionalForm], 
     "errors" -> {}, "input" -> "$l = z R_\\mathrm{m}$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "1a3776d1-2f3a-7041-b37e-979a65db12c8"],
 ",  ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["R", "TI"], 
          StyleBox["m", FontSlant -> "Plain"]], "\[LongEqual]", 
         SubscriptBox[
          StyleBox["l", "TI"], "max"], "\[LongEqual]", "max", 
         RowBox[{"{", 
           RowBox[{
             StyleBox["l", "TI"], ":", "\[Rho]", 
             RowBox[{"(", 
               RowBox[{
                 SubscriptBox[
                  StyleBox["r", "TI"], 
                  StyleBox["l", "TI"]], "(", "\[CurlyTheta]", ")"}], ")"}], 
             ">", "0", "\[ForAll]\[CurlyTheta]"}], "}"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "$R_\\mathrm{m} = l_ {\\max} = \\max\\{l : \\rho (r_l (\\vartheta)) > 0 \
\\forall \\vartheta\\}$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "860c80b0-fa8b-a94a-97f6-e779fb5ead96"],
 ", and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         OverscriptBox["\[Rho]", "_"], "\[LongEqual]", 
         FractionBox[
          StyleBox["M", "TI"], 
          RowBox[{
            FractionBox[
             RowBox[{"4", "\[Pi]"}], "3"], 
            SubsuperscriptBox[
             StyleBox["R", "TI"], 
             StyleBox["m", FontSlant -> "Plain"], "3"]}]]}], TraditionalForm],
      "errors" -> {}, "input" -> 
     "$\\bar{\\rho} = \\frac {M}{\\frac {4\\pi} {3} R_\\mathrm {m}^3}$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cf3e1248-2750-754c-865a-f87dd8461259"],
 " finally yields\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["J", "TI"], 
          RowBox[{"2", 
            StyleBox["n", "TI"]}]], "\[LongEqual]", "-", 
         SuperscriptBox[
          RowBox[{"(", 
            FractionBox[
             SubscriptBox[
              StyleBox["R", "TI"], 
              StyleBox["m", FontSlant -> "Plain"]], 
             SubscriptBox[
              StyleBox["R", "TI"], 
              StyleBox["ref", FontSlant -> "Plain"]]], ")"}], 
          RowBox[{"2", 
            StyleBox["n", "TI"]}]], 
         SubscriptBox[
          StyleBox["S", "TI"], 
          RowBox[{"2", 
            StyleBox["n", "TI"]}]], 
         RowBox[{"(", "1", ")"}]}], TraditionalForm], "errors" -> {}, "input" -> 
     "$J_{2n} = - \
\\left(\\frac{R_\\mathrm{m}}{R_\\mathrm{ref}}\\right)^{2n}S_{2n}(1)$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "13c52b42-5240-f543-bd77-33a2b5f8d823"],
 ".\nFor an actual numerical implementation in python, see \
https://github.com/lucamorf/PyToF."
